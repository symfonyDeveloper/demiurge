{% extends 'base.html.twig' %}
{% block header %}
    <span style="font-size: 22px;">用户添加</span>
{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-lg-12">
            <form role="form" method="post" action="">

                <div class="form-group">
                    <label class="control-label" for="inputSuccess">用户名</label>
                    <input type="text" name="username" class="form-control" id="inputSuccess">
                </div>

                <div class="form-group">
                    <label class="control-label" for="inputWarning">邮箱</label>
                    <input type="text" name="email" class="form-control" id="inputWarning">
                </div>

                <div class="form-group">
                    <label class="control-label" for="inputError">密码</label>
                    <input type="text" name="password" class="form-control" id="inputError">
                </div>
                <button type="submit" class="btn btn-default">提交</button>
                <a href="javascript:history.back(-1)" class="btn btn-default">返回</a>
            </form>
        </div>
    </div>
{% endblock %}